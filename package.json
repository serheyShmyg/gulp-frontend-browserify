  "scripts": {
    "analyze": "yarn dev && npx webpack --profile --json > ./dist/stats.json && npx webpack-bundle-analyzer ./dist/stats.json ./dist --mode=server",
    "start": "yarn webpack serve --color",
    "start:prod": "yarn prod && npx servor ./dist index.html 3001 --reload --browse",
    "dev": "NODE_ENV=development npx webpack --color",
    "prod": "NODE_ENV=production npx webpack --color",
    "clear": "rm -rf dist node_modules",
    "test": "yarn jest --coverage",
    "test:watch": "npx jest --watch --coverage",
    "story": "yarn start-storybook -p 7007",
    "story:build": "npx build-storybook",
    "locale": "NODE_ENV=production npx babel ./src >/dev/null && node ./locale.config.js && rm -rf dist/messages src/locales/whitelists"
  },
  "engines": {
    "node": ">=15"
  },
